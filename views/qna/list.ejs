<%- include('includes/header'); -%>
<%- include('includes/nav'); -%>

<div class="container">
<div class="mx-auto" style="width: 50rem;">

    <div class="container">

        <br>
        <table class="table">
        <tr>
          <th>작성자</th>
          <th>제목</th>
          <th>작성일자</th>
        </tr>

        <% for(var i=0;i < qna.length; i++) { %>
        <tr>
          <td><a href="/qna/<%= qna[i].id %>" ><%= qna[i].id %></a></td>
          <td><a href="/qna/<%= qna[i].id %>" ><%= qna[i].title %></a></td>
          <td><%= moment(qna[i].inserted).format('YYYY-MM-DD h:mm a') %></td>
        </tr>
        <% } %>
        </table>

        <br>
        <a href="/qna/add" class="btn btn-secondary">질문 쓰기</a>
        
        
        
        </div>


